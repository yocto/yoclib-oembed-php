{
  "name": "yocto/yoclib-oembed",
  "type": "library",
  "description": "This yocLibrary enables your project to encode and decode OEmbed data in PHP.",
  "keywords":[
	"composer",
	"json",
	"oembed",
	"php",
	"xml",
	"yocto",
	"yoclib"
  ],
  "license": "GPL-3.0-or-later",
  "require": {
	"php": "^7.1||^8",
	"ext-dom": "*",
	"ext-json": "*"
  },
  "require-dev": {
	"phpunit/phpunit": "^9"
  },
  "autoload": {
	"psr-4": {
	  "YOCLIB\\OEmbed\\": "src/"
	}
  },
  "autoload-dev": {
	"psr-4": {
	  "YOCLIB\\OEmbed\\Tests\\": "tests/"
	}
  },
  "scripts": {
	"test": "phpunit --coverage-clover coverage.xml ."
  }
}
